{
  "request" : {
    "urlPathPattern": "/api/reference/locations",
    "queryParameters" : {
      "filter%5Bcreated_at%5D" : {
        "matches" : "^\\d{4}-\\d{2}-\\d{2}$"
      }
    },
    "method" : "GET"
  },
  "response" : {
    "status" : 200,
    "headers": {
      "Content-Type": "application/json"
    },
    "bodyFileName" : "basm-api/locations/MULTIPLE.json",
    "transformers": [
      "response-template"
    ]
  }
}
